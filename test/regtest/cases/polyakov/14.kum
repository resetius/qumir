цел N=5
целтаб A[1:N]
алг Быстра сортировка
нач
    цел i
    A[1] := 3
    A[2] := 1
    A[3] := 10
    A[4] := 7
    A[5] := 2
    qSort(1, 5)
    нц для i от 0 до N
        вывод A[i], ' '
    кц
кон

алг qSort(цел nStart, nEnd)
нач
    цел L, R, c, X
    если nStart >= nEnd то выход все
    L := nStart; R := nEnd
    X := A[div(L+R, 2)]
    нц пока L <= R
        нц пока A[L]<X; L:=L+1 кц
        нц пока A[R]>X; R:=R-1 кц
        если L <= R то
            c := A[L]; A[L] := A[R]; A[R] := c
            L := L+1; R := R-1
        все
    кц
    qSort(nStart, R)
    qSort(L, nEnd)
кон