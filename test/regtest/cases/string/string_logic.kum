алг цел тест_логика_строки_символы
нач
	лит s1, s2
	сим ch1, ch2

	s1 := "alpha"
	s2 := "alpha"     | equal
	ch1 := a           | symbol implicitly cast to string when compared
	ch2 := z

	цел счет
	счет := 0

	| = equality
	если s1 = s2 то
		счет := счет + 1
	все
	если s1 = ch1 то   | "alpha" == "a" ? false
		счет := счет + 1000 | не должно добавиться
	все
	если ch1 = 'a' то
		счет := счет + 1
	все

	| <> inequality
	если s1 <> s2 то
		счет := счет + 1000 | не должно
	все
	если ch1 <> ch2 то
		счет := счет + 1
	все
	если ch2 <> 'z' то
		счет := счет + 1000 | не должно
	все

	| < <= > >= (lexicographic)
	если s1 < "beta" то
		счет := счет + 1
	все
	если s2 <= "alpha" то
		счет := счет + 1
	все
	если "beta" > s1 то
		счет := счет + 1
	все
	если "alpha" >= s2 то
		счет := счет + 1
	все

	| comparisons with symbols (implicitly strings of length 1)
	если ch1 < ch2 то   | 'a' < 'z'
		счет := счет + 1
	все
	если ch2 > ch1 то
		счет := счет + 1
	все
	если ch1 <= 'a' то
		счет := счет + 1
	все
	если ch2 >= 'z' то
		счет := счет + 1
	все

	знач := счет
кон
