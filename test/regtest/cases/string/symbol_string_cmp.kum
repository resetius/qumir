алг цел тест_сравнение_символ_строка
нач
	сим c
	лит s
	c := 'a'
	s := "a"
	цел r
	r := 0
	если c = s то
		r := r + 1
	все
	если c <> s то
		r := r + 1000 | не должно
	все
	если c <= s то
		r := r + 1
	все
	если c >= s то
		r := r + 1
	все
	если c < "b" то
		r := r + 1
	все
	если c > "`" то   | символ, предшествующий 'a' в ASCII
		r := r + 1
	все
	знач := r
кон
